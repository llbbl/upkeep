{
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "exactOptionalPropertyTypes": false,
    "noImplicitOverride": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler"
  }
}
